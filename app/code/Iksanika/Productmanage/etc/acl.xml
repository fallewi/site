<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Iksanika_Productmanage::config_productmanage" title="Iksanika Advanced Products Manager" sortOrder="10"/>
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Catalog::catalog">
                    <resource id="Magento_Catalog::catalog_inventory">
                        <resource id="Iksanika_Productmanage::productmanage" title="Iksanika Advanced Products Manager" sortOrder="10">
                            <resource id="Iksanika_Productmanage::actions" title="Actions" sortOrder="10">
                                <resource id="Iksanika_Productmanage::ma_delete" title="Delete" sortOrder="10" />
                                <resource id="Iksanika_Productmanage::ma_change_status" title="Change Status" sortOrder="010" />
                                <resource id="Iksanika_Productmanage::ma_update_attributes" title="Update Attributes" sortOrder="020" />
                                <resource id="Iksanika_Productmanage::ma_update" title="Update" sortOrder="030" />
                                <resource id="Iksanika_Productmanage::ma_duplicate" title="Duplicate" sortOrder="040" />
                                <resource id="Iksanika_Productmanage::ma_category_add" title="Category Add" sortOrder="050" />
                                <resource id="Iksanika_Productmanage::ma_category_remove" title="Category Remove" sortOrder="060" />
                                <resource id="Iksanika_Productmanage::ma_category_replace" title="Category Replace" sortOrder="070" />
                                <resource id="Iksanika_Productmanage::ma_update_qty" title="Update Qty" sortOrder="080" />
                                <resource id="Iksanika_Productmanage::ma_update_is_in_stock" title="Update Is In Stock" sortOrder="090" />
                                <resource id="Iksanika_Productmanage::ma_update_price" title="Update Price" sortOrder="100" />
                                <resource id="Iksanika_Productmanage::ma_update_cost" title="Update Cost" sortOrder="110" />
                                <resource id="Iksanika_Productmanage::ma_update_special_price" title="Update Special Price" sortOrder="120" />
                                <resource id="Iksanika_Productmanage::ma_update_price_on_cost" title="Update Price Based on Cost" sortOrder="130" />
                                <resource id="Iksanika_Productmanage::ma_update_special_price_on_cost" title="Update Special Price Based on Cost" sortOrder="140" />
                                <resource id="Iksanika_Productmanage::ma_update_special_price_on_price" title="Update Special Price Based on Price" sortOrder="150" />
                                <resource id="Iksanika_Productmanage::ma_related_to_each_other" title="Related: To Each Other" sortOrder="160" />
                                <resource id="Iksanika_Productmanage::ma_related_to_add" title="Related: Add.." sortOrder="170" />
                                <resource id="Iksanika_Productmanage::ma_related_to_clear" title="Related: Clear" sortOrder="180" />
                                <resource id="Iksanika_Productmanage::ma_cross_sell_to_each_other" title="Cross-Sell: To Each Other" sortOrder="190" />
                                <resource id="Iksanika_Productmanage::ma_cross_sell_to_add" title="Cross-Sell: Add.." sortOrder="200" />
                                <resource id="Iksanika_Productmanage::ma_cross_sell_to_clear" title="Cross-Sell: Clear" sortOrder="210" />
                                <resource id="Iksanika_Productmanage::ma_up_sells_add" title="Up Sell: Add.." sortOrder="220" />
                                <resource id="Iksanika_Productmanage::ma_up_sells_clear" title="Up Sell: Clear" sortOrder="230" />
                            </resource>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
